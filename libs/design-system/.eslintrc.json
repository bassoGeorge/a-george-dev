{
  "extends": [
    "plugin:@nx/react",
    "../../.eslintrc.json",
    "../../.eslintrc.ts.json"
  ],
  "ignorePatterns": [
    "!**/*",
    "dist/**/*",
    "generated-src/**/*",
    ".storybook/preview.js",
    "**/vite.config.*.timestamp*",
    "**/vitest.config.*.timestamp*"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
