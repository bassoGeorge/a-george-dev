{
  "name": "old-remix",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/old-remix",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/.netlify", "{projectRoot}/public/build"],
      "options": {
        "cwd": "apps/old-remix",
        "commands": ["yarn run build"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/old-remix/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/apps/old-remix"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/apps/old-remix"
      }
    }
  }
}
